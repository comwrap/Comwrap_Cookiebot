<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="frame-src">
            <values>
                <value id="cookiebotcdn-framesrc" type="host">https://consentcdn.cookiebot.com</value>
                <value id="cookiebot-framesrc" type="host">https://consent.cookiebot.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="cookiebotcdn-scriptsrc" type="host">https://consentcdn.cookiebot.com</value>
                <value id="cookiebot-scriptsrc" type="host">https://consent.cookiebot.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
